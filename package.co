name                    : 'buildtools'
version                 : '0.1.1'
description             : 'Utilities for use in Jake/Cake/Coke build files.'
homepage                : 'http://dsc.github.com/node-buildtools/'
keywords                : <[ build buildtools jake coke cake tools util server ]>
author                  : 'David Schoonover <dsc@less.ly> (http://less.ly)'

# main                    : './index.js'
# directories             : lib: './lib', bin:'./bin', doc:'./doc', man:'./man'

dependencies            :
    'coco'              : '>= 0.7.0'
    'eyes'              : '>= 0.1.7'
    'colors'            : '>= 0.6.0-1'
    'remove'            : '>= 0.1.1'
    'glob'              : '>= 3.1.6'
    'commondir'         : '>= 0.0.1'
    'cli-table'         : '>= 0.0.1'
    'underscore'        : '>= 1.3.1'
    'underscore.string' : '>= 2.0.0'
    'seq'               : '>= 0.3.5'
    'uglify-js'         : '>= 1.2.5'
    'cssmin'            : '>= 0.3.1'
repository              : type:'git', url:'git://github.com/dsc/node-buildtools.git'
bugs                    : url:'http://github.com/dsc/node-buildtools/issues'

engine                  : node:'>=0.6.2'
license                 : 'MIT'
